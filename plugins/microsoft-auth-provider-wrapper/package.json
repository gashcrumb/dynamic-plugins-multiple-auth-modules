{
  "name": "microsoft-auth-provider-wrapper",
  "version": "0.2.1",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "Apache-2.0",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "backstage": {
    "role": "backend-plugin-module",
    "supported-versions": "1.32.6",
    "pluginId": "plugin-auth-backend-module-microsoft-provider"
  },
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "package.json": [
        "package.json"
      ]
    }
  },
  "scripts": {
    "tsc": "tsc",
    "build": "backstage-cli package build",
    "lint:check": "backstage-cli package lint",
    "test": "backstage-cli package test --passWithNoTests --coverage",
    "clean": "backstage-cli package clean",
    "clean-dynamic-sources": "yarn clean && rm -Rf node_modules",
    "export-dynamic": "janus-cli package export-dynamic-plugin --embed-package @backstage/plugin-auth-backend-module-microsoft-provider --ignore-version-check @backstage/backend-plugin-api",
    "export-dynamic:clean": "run export-dynamic --clean"
  },
  "dependencies": {
    "@backstage/backend-plugin-api": "1.0.1",
    "@backstage/plugin-auth-backend-module-microsoft-provider": "0.2.1"
  },
  "devDependencies": {
    "@backstage/cli": "0.28.2",
    "@janus-idp/cli": "1.18.6",
    "typescript": "5.6.3"
  },
  "files": [
    "dist",
    "dist-dynamic/*.*",
    "dist-dynamic/dist/**"
  ]
}
