{
  "name": "custom-authentication-provider-module",
  "version": "0.0.1",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "Apache-2.0",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "backstage": {
    "role": "frontend-plugin",
    "supported-versions": "1.32.6",
    "pluginId": "custom-authentication-provider-module",
    "pluginPackages": [
      "custom-authentication-provider-module",
      "custom-authentication-provider-module-backend"
    ]
  },
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "package.json": [
        "package.json"
      ]
    }
  },
  "scripts": {
    "tsc": "tsc",
    "build": "backstage-cli package build",
    "lint:check": "backstage-cli package lint",
    "test": "backstage-cli package test --passWithNoTests --coverage",
    "clean": "backstage-cli package clean",
    "clean-dynamic-sources": "yarn clean && rm -Rf node_modules",
    "export-dynamic": "janus-cli package export-dynamic-plugin",
    "export-dynamic:clean": "run export-dynamic --clean"
  },
  "dependencies": {
    "@backstage/core-app-api": "1.15.1",
    "@backstage/core-components": "0.15.1",
    "@backstage/core-plugin-api": "1.10.0",
    "@backstage/integration-react": "1.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@backstage/cli": "0.28.2",
    "@janus-idp/cli": "1.18.6",
    "@types/react": "^18.0.0",
    "react-router": "^6.4.5",
    "react-router-dom": "^6.4.5",
    "typescript": "5.6.3"
  },
  "peerDependencies": {
    "react": "^16.13.1 || ^17.0.0 || ^18.0.0"
  },
  "files": [
    "dist",
    "dist-dynamic/*.*",
    "dist-dynamic/dist/**"
  ]
}
